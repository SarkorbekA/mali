mixin program()
  section.program#program
    .program-item.first
      img(src="@img/pelmeni/item1.png" alt="item" draggable="false")
    .program-item.second
      img(src="@img/pelmeni/item2.png" alt="item" draggable="false")
    .program-wrapper
      .program-left 
        h3.program-title  Hamkorlik dasturi
        p.program-text Mali bilan yetakchi chakana savdo tarmoqlari va HoReCa sanoati vakillari faol hamkorlik qilmoqda, jumladan:
        .program__left-box
          ul.program__list 
            li.program__list-item.first 
              img(src="@img/partners/el1.png" alt="partner")
              img(src="@img/partners/el3.png" alt="partner").inner
            li.program__list-item.second
              img(src="@img/partners/el2.png" alt="partner")
              img(src="@img/partners/el4.png" alt="partner").inner
          a(href="/catalog-uz.html").button-other.white 
            h3 Barcha mahsulotlar
